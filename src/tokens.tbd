rule
    : token = word{value}
    : value = string{value}
as
    : as = '{' word{value} '}'
step
    : step = rule{rule} as{as}? qualifier{qualifier}?
pattern
    : pattern = step{rules}*
subtype
    : subtype = colon word{name} equal pattern{pattern} newline
type
    : type = word{name} newline subtype{subtypes}*
file
    : file = type{types}*
