parenthesis = ('(' | ')')
braces      = ('{' | '}')
brackets    = ('[' | ']')

not = '!'
and = '&'
or  = '|'

equal = '='
colon = ':'

qualifier = ('*' | '+' | '?' | '^')

newline:skip = (\n | \r)
whitespace:skip = (\s | \t)

char:basic_char = ''' !newline '''
char:special_char = '\' !(newline | whitespace)

string:string = '"' !(newline | '"') '"'

word = (('a' to 'z') | ('A' to 'Z') | '_' | '-')+

comment:skip = "(*" !"*)"* "*)"